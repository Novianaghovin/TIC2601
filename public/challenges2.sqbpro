"activity_log" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="68"/><column index="2" value="90"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">CREATE TABLE USER_PROFILE (
    user_id INTEGER PRIMARY KEY,       -- Unique user ID
    dob DATE NOT NULL,                 -- Date of birth
    gender TEXT CHECK(gender IN ('Male', 'Female', 'Other')),  -- Gender, restricted to specific values
    height DECIMAL(5, 2),             -- Height in meters (e.g., 1.75)
    weight DECIMAL(5, 2),              -- Weight in kilograms (e.g., 70.50)
    nationality TEXT NOT NULL          -- Nationality of the user
);

